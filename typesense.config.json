{
  "index_name": "emacs-docs",
  "allowed_domains": ["emacsdocs.org"],
  "start_urls": [
    {
      "url": "https://www.emacsdocs.org/"
    }
  ],
  "selectors": {
    "default": {
      "lvl0": "article h1",
      "lvl1": "article h2",
      "lvl2": "article h3",
      "lvl3": "article h4",
      "text": "article p, article ul li, article table tbody tr"
    }
  },
  "scrape_start_urls": false,
  "strip_chars": " .,;:#"
}
